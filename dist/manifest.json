{
  "manifest_version": 3,
  "name": "Parrot AI Interviewer",
  "version": "1.0",
  "description": "Turn any LeetCode problem into a mock interview",
  "action": {
    "default_popup": "index.html",
    "default_icon": {
      "16": "Parrot.png",
      "32": "Parrot.png",
      "48": "Parrot.png",
      "128": "Parrot.png"
    }
  },
  "permissions": ["storage", "tabs", "activeTab", "scripting"],
  "host_permissions": ["https://leetcode.com/problems/*"]
}